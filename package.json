{
  "name": "angular2-memory-game",
  "version": "0.0.0",
  "description": "A tiny game written in `angular2`. It's inspired by IgorMinar's Memory-Game",
  "scripts": {
    "start": "ng serve",
    "dist": "ng build --aot -vendor-chunk --target production --environment prod",
    "deploy": "rm -rf dist && npm run dist && git checkout gh-pages && rm -f *.* && mv dist/* .",
    "lint": "ng lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leftstick/angular2-memory-game.git"
  },
  "keywords": [
    "angular",
    "memory-game"
  ],
  "author": "Howard.Zuo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leftstick/angular2-memory-game/issues"
  },
  "homepage": "https://github.com/leftstick/angular2-memory-game#readme",
  "dependencies": {
    "@angular-redux/store": "^6.5.7",
    "@angular/common": "^4.3.3",
    "@angular/compiler": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@angular/platform-browser": "^4.3.3",
    "@angular/platform-browser-dynamic": "^4.3.3",
    "core-js": "^2.5.0",
    "redux": "^3.7.2",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "1.2.7",
    "@angular/compiler-cli": "^4.3.3",
    "@types/node": "~8.0.20",
    "codelyzer": "~3.1.2",
    "tslint": "~5.6.0",
    "typescript": "~2.4.2"
  }
}
